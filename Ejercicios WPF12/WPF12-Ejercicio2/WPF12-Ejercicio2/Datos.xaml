<Window x:Class="WPF12_Ejercicio2.Datos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF12_Ejercicio2"
        mc:Ignorable="d"
        Title="Datos" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox IsReadOnly="True" Name="ResConsulta" TextAlignment="Center" Margin="10" Visibility="Hidden" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Button Content="Cerrar" FontWeight="Bold" Margin="10" Width="200" Height="50" Click="Close"/>
        </StackPanel>
    </Grid>
</Window>
